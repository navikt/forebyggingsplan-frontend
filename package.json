{
    "name": "forebyggingsplan-frontend",
    "version": "0.1.0",
    "private": true,
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamia"
        ],
        "overrides": [
            {
                "files": [
                    "sanity-studio/*"
                ],
                "extends": [
                    "sanity/react",
                    "sanity/typescript",
                    "prettier"
                ],
                "rules": {
                    "no-process-env": "off"
                }
            }
        ],
        "ignorePatterns": [
            "*.module.css"
        ]
    },
    "scripts": {
        "mock": "NAIS_APP_NAME=forebyggingsplan-frontend-mock next dev",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:ci": "jest --ci",
        "prepare": "husky install"
    },
    "dependencies": {
        "@amplitude/analytics-browser": "1.6.7",
        "@navikt/bedriftsmeny": "6.4.0",
        "@navikt/ds-css": "4.1.7",
        "@navikt/ds-icons": "3.4.3",
        "@navikt/ds-react": "4.1.7",
        "@navikt/nav-dekoratoren-moduler": "1.9.0",
        "@portabletext/react": "2.0.1",
        "@sanity/client": "5.2.2",
        "@sanity/image-url": "1.0.2",
        "jose": "4.12.0",
        "next": "13.1.6",
        "openid-client": "5.4.0",
        "pino": "^8.11.0",
        "pino-pretty": "^9.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.8.1",
        "swr": "2.0.3"
    },
    "devDependencies": {
        "@navikt/eslint-config-teamia": "1.0.5",
        "@sanity/eslint-config-studio": "2.0.1",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/jest-axe": "3.5.5",
        "@types/node": "18.14.1",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "eslint-config-sanity": "6.0.0",
        "husky": "8.0.3",
        "jest": "29.3.1",
        "jest-axe": "7.0.0",
        "jest-environment-jsdom": "29.3.1",
        "lint-staged": "13.0.4",
        "msw": "1.0.1",
        "ts-node": "10.9.1",
        "typescript": "4.9.5",
        "whatwg-fetch": "3.6.2"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css}": [
            "prettier --write",
            "eslint --fix"
        ]
    }
}
